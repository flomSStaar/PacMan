<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.VueMap">
    <top>
        <HBox>
            <children>
                <Text fx:id="score" text="Score" />
                <Text strokeType="OUTSIDE" strokeWidth="0.0" />
            </children>
        </HBox>
    </top>
    <center>
        <StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
            <ImageView pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="/image/map.png" />
                </image>
            </ImageView>
        </StackPane>
    </center>
</BorderPane>
